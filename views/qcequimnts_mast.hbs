        </div>
        </form>
  </div>
      </section>
    </div>
    <div class="col-md-7">
      <section class="content-header">
        <div class="box box-primary">
          <div class="box-header with-border">
            <h3 class="box-title">View Q.C Equipments Master </h3>
                  <p style="text-align: center;color:#ff0000;margin-top: -20px;"></p>
          </div>
            <div class="box-body">
              <div class="table-responsive">
                <table id="example" class="table table-bordered table-striped">
                  <thead>
                      <tr>
                        <th>#</th>
                        <th>Eq. Name</th>
                        <th>Calibration</th>
                        <th>Calibration</th>
                        <th>Calibration Sched.</th>
                        <th>Department</th>
                        <th>Weight Box Calibration</th>
                        <th>Check List</th>
                        <th>Instructions</th>
                        <th width="10%">Update</th>
                        <th width="10%">Delete</th>
                      </tr>
                  </thead>
                 <tbody>
                   {{#each qcequimnts as |value key|}}
                      <tr>
                          <td>{{sum @index 1}}</td>
                          <td>{{this.qcequimnts_eqname}}</td>
                          <td>{{this.qcequimnts_calbrton}}</td>
                          <td>{{this.qcequimnts_calbrtontwo}}</td>
                          <td>{{this.qcequimnts_days}}</td>
                          <td>{{this.qcequimnts_deprt.departmnt_name}}</td>
                          <td>{{this.qcequimnts_wghtbxcabrton}}</td>
                          <td>{{this.qcequimnts_chklst.machinechk_dscrpton}}</td>
                          <td>{{this.qcequimnts_instrcton}}</td>
                          <td><a href="javascript:void(0);"  class="edit_qcequimnts" data-id="{{this._id}}"><i class="fa fa-pencil"></i></i></a></td>
                          <td><a href="/qcequimnts_mast/delete_qcequimnts/{{value._id}}" onclick="return confirm('Are you sure want to delete.');"><i class="fa fa-trash-o" style="color:red;"></i></a></td>
                      </tr>
                     {{/each}}  
                   </tbody>
                </table>
      </div>
    </div>
  </div>
      </section>
    </div>
    
  </div>
</div>
