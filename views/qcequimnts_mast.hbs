  <div class="box box-primary">
    <div class="box-header with-border">
      <h3 class="box-title" id="add">Q.C Equipments Master</h3>
      <h3 class="box-title" id="edit" style="display: none">Update Q.C Equipments Master</h3>
          {{#if err}}
          <div class="alert alert-danger">
          <strong>{{err}}</strong>
          </div>
          {{/if}}
          {{#if errors}}
          <div class="alert alert-danger">
          <strong>{{errors}}</strong>
          </div>
          {{/if}}
      <div class="box-tools pull-right">
            <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
            <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-remove"></i></button>
          </div>
    </div>
       <form action="/qcequimnts_mast/add" class="form-horizontal"  method="post" id="qcequimnts_add">
          <h4 style="color: red;text-align: center;"></h4>
        <div class="box-body">
          <div class="form-group">
            <label class="col-md-4">Eq. Name</label>
            <div class="col-md-8"> 
                <input type="text" class="form-control" name="qcequimnts_eqname" id="qcequimnts_eqname" placeholder="Eq. Name">
            </div>
          </div>
          <div class="form-group">
            <label class="col-md-4">Calibration</label>
            <div class="col-md-8"> 
                 <select class="form-control" name="qcequimnts_calbrton" id="qcequimnts_calbrton">
                 <option value="">----Select Calibration----</option>
                 <option value="Y">Y</option>
                 <option value="N">N</option>
              </select>
            </div>
          </div>
          <div class="form-group">
            <label class="col-md-4">Calibration</label>
            <div class="col-md-8"> 
                 <select class="form-control" name="qcequimnts_calbrtontwo" id="qcequimnts_calbrtontwo">
                 <option value="">----Select Calibration----</option>
                 <option value="Internal">Internal</option>
                 <option value="Extrnal">Extrnal</option>
              </select>
            </div>
          </div>
           <div class="form-group">
            <label class="col-md-4">Calibration Sched.</label>
            <div class="col-md-4"> 
                <input type="text" class="form-control" name="qcequimnts_days" id="qcequimnts_calbrton" placeholder="Days">
            </div>
            <div class="col-md-4"> 
                <input type="text" class="form-control" name="qcequimnts_year" id="qcequimnts_year" placeholder="Year">
            </div>
          </div>
          <div class="form-group">
            <label class="col-md-4">Department</label>
            <div class="col-md-8"> 
               <select class="form-control qcequimnts_deprt" name="qcequimnts_deprt" id="qcequimnts_deprt">
                 <option value="">----Select Department----</option>
                {{#each depart}}
                <option value="{{this._id}}" data-departmnt="{{this._id}}">{{this.departmnt_name}}</option>
                {{/each}}
              </select>
            </div>
          </div>
          <div class="form-group">
            <label class="col-md-4">Weight Box Calibration</label>
            <div class="col-md-8"> 
                <input type="checkbox" name="qcequimnts_wghtbxcabrton" id="qcequimnts_wghtbxcabrton" value="Y" required>
            </div>
          </div>
           <div class="form-group">
            <label class="col-md-4">Check List</label>
            <div class="col-md-8"> 
               <select class="form-control qcequimnts_chklst" name="qcequimnts_chklst" id="qcequimnts_chklst">
                 <option value="">----Select Check List----</option>
                {{#each machinechecklist}}
                <option value="{{this._id}}" data-departmnt="{{this._id}}">{{this.machinechk_dscrpton}}</option>
                {{/each}}
              </select>
            </div>
          </div>
           <div class="form-group">
            <label class="col-md-4">Instructions</label>
            <div class="col-md-8"> 
                <input type="text" class="form-control" name="qcequimnts_instrcton" id="qcequimnts_instrcton" placeholder="Instructions">
            </div>
          </div>
        </div>
        <!-- /.box-body -->
        <div class="box-footer">
           <input type="reset" class="btn btn-default" value="Clear Form">
              <input type="submit" class="btn btn-info pull-right" value="submit" id="submit">
              <input type="submit" class="btn btn-success pull-right"  value="update" id="update" style="display:none;">
        </div>
        </form>
