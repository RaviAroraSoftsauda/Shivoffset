<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
  <!-- Content Header (Page header) -->
  <div class="row">
    <div class="col-md-12">
      <section class="content-header">
         
         
  <div class="box box-primary">
    <div class="box-header with-border">
      <h3 class="box-title" id="add">Update Plate Issuance Note</h3>
          {{#if err}}
          <div class="alert alert-danger">
          <strong>{{err}}</strong>
          </div>
          {{/if}}
          {{#if errors}}
          <div class="alert alert-danger">
          <strong>{{errors}}</strong>
          </div>
          {{/if}}
      <div class="box-tools pull-right">
            <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
            <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-remove"></i></button>
          </div>
    </div>
       <form action="/plate_receval_note/update/{{plateissuance.id}}" class="form-horizontal" method="post" id="supplier_add">
          <h4 style="color: red;text-align: center;"></h4>
        <div class="box-body">
          <div class="form-group">
             <label class="col-md-3">Enter Book/No</label>
            <div class="col-md-2"> 
                <input type="number" class="form-control" name="ref_no" id="ref_no" value="{{plateissuance.ref_no}}" required="">
            </div>
             <div class="col-md-2"> 
                <input type="text" class="form-control" name="c_j_s_p" id="c_j_s_p"  value="{{plateissuance.c_j_s_p}}">
            </div>
            <label class="col-md-2">Date</label>
            <div class="col-md-3"> 
                <input type="text" class="form-control" name="acc_date" id="filterstartdate" value="{{plateissuance.acc_date}}" placeholder="Date" required="">
            </div>
          </div>
           <div class="form-group">
             <label class="col-md-3">From Location</label>
            <div class="col-md-4"> 
               {{#listrtcomp city plateissuance.from_ltcon  'from_ltcon' 'from_ltcon'}}
                            {{this.city_name}}
                  {{/listrtcomp}} 
               {{!-- <select class="form-control" name='from_ltcon'  id='from_ltcon'>
                    <option value="">-Select From Location-</option>
                    {{#each city}}
                    <option value="{{this._id}}" data-city="{{this._id}}">{{this.city_name}}</option>
                    {{/each}}
                   </select> --}}
            </div>
            <label class="col-md-2">To Location</label>
            <div class="col-md-3"> 
              {{#listrtcomp city plateissuance.to_ltcon  'to_ltcon' 'to_ltcon'}}
                            {{this.city_name}}
                  {{/listrtcomp}} 
                {{!-- <select class="form-control" name='to_ltcon'  id='to_ltcon'>
                    <option value="">-Select To Location-</option>
                    {{#each city}}
                    <option value="{{this._id}}" data-city="{{this._id}}">{{this.city_name}}</option>
                    {{/each}}
                   </select> --}}
            </div>
          </div>
          <div class="form-group">
            <div class="col-md-12">
              <table>
                <thead style="background-color:#dd4b39;color: #fff;font-size: 15px;">
                  <tr>
                    <th>Job Card No</th>
                    <th>Prod Code</th>
                    <th>Prod Name</th>
                    <th>Customer</th>
                    <th>Type CTP/TS</th>
                    <th>O/N old\new</th>
                    <th>plate size</th>
                    <th>No of plates</th>
                    <th>Remarks</th>
                    <th>Qty</th>
                    <th>Location</th>
                    <th>Delete</th>
                  </tr>
                </thead>
               <tbody>
                 {{#each plateissuance.p_i_n}}
              <tr class="report_variation_wrapper" id="report{{@index}}">
                <td><input type='text' class="form-control" name='p_i_n[{{@index}}][jobcrdno]' onkeyup="fname({{@index}});" value="{{this.jobcrdno}}"  id='jobcrdno{{@index}}' style='width:100%;' placeholder="Job Card No"></td>
                <td class="item_code{{@index}}"><input type='text' class="form-control" name='p_i_n[{{@index}}][procode]' value="{{this.procode}}"  id='procode' style='width:100%;' placeholder="Prod Code"></td>
                <td class="produtname{{@index}}"><input type='text' class="form-control" name='p_i_n[{{@index}}][prdtname]' value="{{this.prdtname}}"  id='prdtname' style='width:100%;' placeholder="Prod Name"></td>
                <td class="partynm{{@index}}"><input type='text' class="form-control" name='p_i_n[{{@index}}][custumer]' value="{{this.custumer}}" id='custumer' style='width:100%;' placeholder="Customer"></td>
                <input type="hidden" name="cntrer{{@index}}" value="{{@index}}" id="cntrer{{@index}}" class="form-control" style="width: 102px;">
                <td><input type='text' class="form-control" name='p_i_n[{{@index}}][typctpts]' value="{{this.typctpts}}" id="typctpts"  style='width:100%;' placeholder="Type CTP/TS"></td>
                <td><input type='text' class="form-control" name='p_i_n[{{@index}}][onldnw]' value="{{this.onldnw}}"  id='onldnw' style='width:100%;' placeholder="O/N old\new"></td>
                <td>
                   <select class="form-control" name='p_i_n[{{@index}}][platesz]'  id='platesz' style='width:100%;'>
                    <option value="">-Select Plate Size-</option>
                    {{#each ../accessories}}
                    <option value="{{this._id}}" data-accessories="{{this._id}}" {{#equal ../platesz this._id}}selected=""{{/equal}}>{{this.accessories_desc}}</option>
                    {{/each}}
                   </select>
                  </td>
                <td><input type='text' class="form-control" name='p_i_n[{{@index}}][nofplates]' value="{{this.nofplates}}"  id='nofplates' style='width:100%;' placeholder="No of plates"></td>
                <td><input type='text' class="form-control" name='p_i_n[{{@index}}][remarks]' value="{{this.remarks}}" id='remarks' style='width:100%;' placeholder="Remarks"></td>
                <td><input type='text' class="form-control" name='p_i_n[{{@index}}][qty]' value="{{this.qty}}"  id='qty' style='width:100%;' placeholder="Qty"></td>
                <td>
                   <select class="form-control" name='p_i_n[{{@index}}][location]'  id='location' style='width:100%;'>
                    <option value="">-Select Location-</option>
                  {{#each ../recepit as |recepitvalue key|}}
                    {{#each recepitvalue.acc_note_item as |accvalue key|}}
                    <option value="{{accvalue.location}}" data-accessories="{{accvalue.location}}" {{#equal ../location this._id}}selected=""{{/equal}}>{{accvalue.location}}</option>
                  {{/each}}
                  {{/each}}
                  </select>
                      <td><a onclick='remove_report({{@index}});' style='cursor:pointer;'><button type="button" class="btn btn-success">Select</button></a></td>
                  
                
        </tr>
        {{/each}}
         <tr  id="insert_stockitem">
               </tr>
      </tbody>
      <tr>
      </tr>
    </table>
    </div> 
    </div>
    {{!-- <center><a class="addpin" style="font-size: 30px;cursor:pointer;"><i class="fa fa-plus" aria-hidden="true"></i></a></center> --}}

        </div>
        <!-- /.box-body -->
        <div class="box-footer">
           <input type="submit" class="btn btn-success" value="submit" id="submit">
           <input type="reset" class="btn btn-default pull-right" value="Clear Form">  
        </div>
        </form>
  </div>
      </section>
    </div>
  </div>
</div>
 {{> footer}}
<style>
 .button-clear {

          background-color: Transparent;
            background-repeat:no-repeat;
            border: none;
            cursor:pointer;
            overflow: hidden;

        }
       </style>
       