{{> header}}
{{> nav}}
<div class="content-wrapper">
  <section class="content-header">
     {{!-- <a href="/product_mast/product_mast" style="color:white"><h1 class="btn btn-primary pull-left" style="margin-bottom: 21px;">Add job list</h1></a> --}}
      <ol class="breadcrumb" >
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
         <li><a href="#"><i class="fa fa-dashboard"></i> Add job list</a></li>
      </ol>
    </section>
  <div class="row">
    <div class="col-md-7">
      <section class="content-header">
          {{#if err}}
          <div class="alert alert-danger">
          <strong>{{err}}</strong>
          </div>
          {{/if}}
          {{#if errors}}
          <div class="alert alert-danger">
          <strong>{{errors}}</strong>
          </div>
          {{/if}}
      </section>
    </div>
    <div class="col-md-12">
      <section class="content-header">
        <div class="box box-primary">
          <div class="box-header with-border">
            <div class="box-tools pull-right">
            <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
            <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-remove"></i></button>
          </div>
            <h3 class="box-title">View job list</h3>
                  <p style="text-align: center;color:#ff0000;margin-top: -20px;"></p>
          </div>
            <div class="box-body">
              <div class="table-responsive">
                <table id="example" class="table display table-bordered table-striped">
                  <thead>
                      <tr>
                        <th>#</th>
                        <th>Item Code</th>
                        <th>Delv At</th>
                        <th>Job Card No</th>
                        <th>Po. No</th>
                        <th>Po. Date</th>
                        {{!-- <th>Party Name</th> --}}
                        <th>Lamination</th>
                        <th>Varnish</th>
                        <th>GSM</th>
                        <th>Material</th>
                        <th>Cut Size</th>
                        <th>Piching/Pasting</th>
                        <th>Actual Sheet</th>
                        <th>Varnish UV</th>
                        <th>Fold/Flat Size</th>
                        <th>Folding/Flat</th>
                        <th>UPS</th>
                        <th>Remarks</th>
                        <th>Stock in hand</th>
                        <th>Design style</th>
                        <th>Location</th>
                        <th>Send Shade Card</th>
                        <th>Change</th>
                        <th>New Job</th>
                        <th>Repeat</th>
                         <th>TP</th>
                        <th width="10%">Update</th>
                        <th width="10%">Delete</th>
                      </tr>
                  </thead>
                 <tbody>
                   {{#each newjobentry as |value key|}}
                      <tr>
                          <td>{{sum @index 1}}</td>
                          <td>{{this.newjb_itemcd}}</td>
                          <td>{{this.newjb_dlvat.city_name}}</td>
                          <td>{{this.newjb_jbcrd}}</td>
                          <td>{{this.newjb_pono}}</td>
                          <td>{{this.newjb_podt}}</td>
                          {{!-- <td>{{this.newjb_prtynm.prdt_prtyname}}</td> --}}
                          <td>{{this.newjb_lmiton}}</td>
                          <td>{{this.newjb_vernish.vernish_name}}</td>
                          <td>{{this.newjb_gsm}}</td>
                          <td>{{this.newjb_matril}}</td>
                          <td>{{this.newjb_cutsz}}</td>
                          <td>{{this.newjb_pichng}}</td>
                          <td>{{this.newjb_actulsz}}</td>
                          <td>{{this.newjb_vrnshuv}}</td>
                          <td>
                          <table>
                            <tr>
                              <td>Length:</td>
                              <td>{{this.newjb_folfltszl}}</td>
                            </tr>
                            <tr>
                              <td>Width:</td>
                              <td>{{this.newjb_folfltszw}}</td>
                            </tr>
                             <tr>
                              <td>Height:</td>
                              <td>{{this.newjb_folfltszh}}</td>
                            </tr>
                          </table>
                          </td>
                          <td>{{this.newjb_folft.indsrttyp_dscrpton}}</td>
                          <td>{{this.newjb_ups}}</td>
                          <td>{{this.newjb_remks}}</td>
                          <td>{{this.newjb_stckhnd}}</td>
                          <td>{{this.newjb_dsignstyl.design_style}}</td>
                          <td>{{this.newjb_locton.city_name}}</td>
                          <td>{{this.newjb_sndshdcrd}}</td>
                          <td>{{this.newjb_chng}}</td>
                          <td>{{this.newjb_nwjb}}</td>
                          <td>{{this.newjb_rupes}}</td>
                          <td>{{this.newjb_tp}}</td>
                          <td><a href="/newjobentry/newjobentry_update/{{value._id}}"  class="edit_prodtmast" data-id="{{this._id}}"><i class="fa fa-pencil"></i></i></a></td>
                          <td><a href="/newjobentry/delete_newjobentry/{{value._id}}" onclick="return confirm('Are you sure want to delete.');"><i class="fa fa-trash-o" style="color:red;"></i></a></td>
                      </tr>
                     {{/each}}  
                   </tbody>
                </table>
      </div>
    </div>
  </div>
      </section>
    </div>
    
  </div>
</div>
 {{> footer}}
<style>
 .button-clear {

          background-color: Transparent;
            background-repeat:no-repeat;
            border: none;
            cursor:pointer;
            overflow: hidden;

        }
        div.dataTables_wrapper {
        margin-bottom: 3em;
    }
       </style>
       