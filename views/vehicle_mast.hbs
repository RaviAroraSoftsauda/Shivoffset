<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
  <!-- Content Header (Page header) -->
  <div class="row">
    <div class="col-md-5">
      <section class="content-header">
         
         
  <div class="box box-primary">
    <div class="box-header with-border">
      <h3 class="box-title" id="add">Vehicle Master</h3>
      <h3 class="box-title" id="edit" style="display: none">Update Vehicle Master</h3>
          {{#if err}}
          <div class="alert alert-danger">
          <strong>{{err}}</strong>
          </div>
          {{/if}}
          {{#if errors}}
          <div class="alert alert-danger">
          <strong>{{errors}}</strong>
          </div>
          {{/if}}
      <div class="box-tools pull-right">
            <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
            <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-remove"></i></button>
          </div>
    </div>
       <form action="/vehicle_mast/add" class="form-horizontal"  method="post" id="vehicle_add">
          <h4 style="color: red;text-align: center;"></h4>
        <div class="box-body">
          <div class="form-group">
            <label class="col-md-4">Vehicle No</label>
            <div class="col-md-8"> 
                <input type="text" class="form-control" name="vehicle_no" id="vehicle_no" placeholder="Vehicle No">
            </div>
            <label class="col-md-4">Company Maker </label>
            <div class="col-md-8"> 
                <input type="text" class="form-control" name="vehicle_cmpnymakr" id="vehicle_cmpnymakr" placeholder="Company Maker">
            </div>
            <label class="col-md-4">Model No </label>
            <div class="col-md-8"> 
                <input type="text" class="form-control" name="vehicle_mdlno" id="vehicle_mdlno" placeholder="Model No">
            </div>
            <label class="col-md-4">Our/Atteched</label>
            <div class="col-md-8"> 
                <select class="form-control vehicle_orarrchd" name="vehicle_orarrchd" id="vehicle_orarrchd">
                 <option value="">Select Our/Atteched</option>
                  <option value="Our">Our</option>
                   <option value="Atteched">Atteched</option>
              </select>
            </div>
            <label class="col-md-4">No. Of Axels</label>
            <div class="col-md-8"> 
                <input type="text" class="form-control" name="vehicle_nofaxels" id="vehicle_nofaxels" placeholder="No. Of Axels">
            </div>
            <label class="col-md-4">R.T.O Passing</label>
            <div class="col-md-8"> 
                <input type="text" class="form-control" name="vehicle_rtopssing" id="vehicle_rtopssing" placeholder="R.T.O Passing">
            </div>
            <label class="col-md-4">Chasis No</label>
            <div class="col-md-8"> 
                <input type="text" class="form-control" name="vehicle_chisno" id="vehicle_chisno" placeholder="Chasis No">
            </div>
            <label class="col-md-4">Engine No</label>
            <div class="col-md-8"> 
                <input type="text" class="form-control" name="vehicle_engno" id="vehicle_engno" placeholder="Engine No">
            </div>
            <label class="col-md-4">Active</label>
            <div class="col-md-8"> 
                <select class="form-control vehicle_active" name="vehicle_active" id="vehicle_active">
                 <option value="">Select Active</option>
                  <option value="Y">Y</option>
                   <option value="N">N</option>
              </select>
            </div>
            <label class="col-md-4">Expen. Win </label>
            <div class="col-md-8"> 
                <select class="form-control vehicle_expwin" name="vehicle_expwin" id="vehicle_expwin">
                 <option value="">Select Expen. Win</option>
                  <option value="Y">Y</option>
                   <option value="N">N</option>
              </select>
            </div>
            <label class="col-md-4">Lock Dt</label>
            <div class="col-md-8"> 
                <input type="text" class="form-control"  value="{{dateformat Date.now 'DD/MM/YYYY'}}" name="vehicle_lockdt" id="filterstartdate">
            </div>
            <label class="col-md-4">Mfg Dt</label>
            <div class="col-md-8"> 
                <input type="text" class="form-control" value="{{dateformat Date.now 'DD/MM/YYYY'}}" name="vehicle_mfgdt" id="filterenddate">
            </div>
            <label class="col-md-4">Other Info</label>
            <div class="col-md-8"> 
                <input type="text" class="form-control" name="vehicle_othrinfo" id="vehicle_othrinfo" placeholder="Other Info">
            </div>
            <label class="col-md-4">Owner Name</label>
            <div class="col-md-8"> 
                <input type="text" class="form-control" name="vehicle_onwrnm" id="vehicle_onwrnm" placeholder="Owner Name">
            </div>
            <label class="col-md-4">Pay Load</label>
            <div class="col-md-8"> 
                <input type="text" class="form-control" name="vehicle_pylod" id="vehicle_pylod" placeholder="Pay Load">
            </div>
        </div>
        </div>
        <!-- /.box-body -->
        <div class="box-footer">
           <input type="reset" class="btn btn-default" value="Clear Form">
              <input type="submit" class="btn btn-info pull-right" value="submit" id="submit">
              <input type="submit" class="btn btn-success pull-right"  value="update" id="update" style="display:none;">
        </div>
        </form>
