              </select>
            </div>
            <label class="col-md-4">Lock Dt</label>
            <div class="col-md-8"> 
                <input type="text" class="form-control vehicle_lockdt"  name="vehicle_lockdt" id="filterstartdateup">
            </div>
            <label class="col-md-4">Mfg Dt</label>
            <div class="col-md-8"> 
                <input type="text" class="form-control vehicle_lockdt"  name="vehicle_mfgdt" id="filterenddateup">
            </div>
            <label class="col-md-4">Other Info</label>
            <div class="col-md-8"> 
                <input type="text" class="form-control vehicle_othrinfo" name="vehicle_othrinfo" id="vehicle_othrinfo" placeholder="Other Info">
            </div>
            <label class="col-md-4">Owner Name</label>
            <div class="col-md-8"> 
                <input type="text" class="form-control vehicle_onwrnm" name="vehicle_onwrnm" id="vehicle_onwrnm" placeholder="Owner Name">
            </div>
            <label class="col-md-4">Pay Load</label>
