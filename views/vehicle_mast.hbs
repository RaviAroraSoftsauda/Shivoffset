                <input type="text" class="form-control vehicle_pylod" name="vehicle_pylod" id="vehicle_pylod" placeholder="Pay Load">
            </div>
        </div>
        </div>
        <!-- /.box-body -->
        <div class="box-footer">
           <input type="reset" class="btn btn-default" value="Clear Form">
              <input type="submit" class="btn btn-success pull-right"  value="update" id="update">
        </div>
        </form>
  </div>
      </section>
    </div>
    <div class="col-md-7">
      <section class="content-header">
        <div class="box box-primary">
          <div class="box-header with-border">
            <h3 class="box-title">View Vehicle Master </h3>
                  <p style="text-align: center;color:#ff0000;margin-top: -20px;"></p>
          </div>
            <div class="box-body">
              <div class="table-responsive">
                <table id="example" class="table table-bordered table-striped">
                  <thead>
                      <tr>
                        <th>#</th>
                        <th>Vehicle No</th>
                        <th>Company Maker</th>
                        <th>Model No</th>
                        <th>Our/Atteched</th>
                        <th>No. Of Axels</th>
                        <th>R.T.O Passing</th>
                        <th>Chasis No</th>
                        <th>Engine No</th>
                        <th>Active</th>
                        <th>Expen. Win</th>
                        <th>Lock Dt</th>
                        <th>Mfg Dt</th>
                        <th>Other Info</th>
                        <th>Owner Name</th>
                        <th>Pay Load</th>
                        <th width="10%">Update</th>
                        <th width="10%">Delete</th>
                      </tr>
                  </thead>
                 <tbody>
                   {{#each vehicle as |value key|}}
                      <tr>
                          <td>{{sum @index 1}}</td>
                          <td>{{this.vehicle_no}}</td>
                          <td>{{this.vehicle_cmpnymakr}}</td>
                          <td>{{this.vehicle_mdlno}}</td>
                          <td>{{this.vehicle_orarrchd}}</td>
                          <td>{{this.vehicle_nofaxels}}</td>
                          <td>{{this.vehicle_rtopssing}}</td>
                          <td>{{this.vehicle_chisno}}</td>
                          <td>{{this.vehicle_engno}}</td>
                          <td>{{this.vehicle_expwin}}</td>
                          <td>{{this.vehicle_active}}</td>
                          <td>{{this.vehicle_lockdt}}</td>
                          <td>{{this.vehicle_mfgdt}}</td>
                          <td>{{this.vehicle_othrinfo}}</td>
                          <td>{{this.vehicle_onwrnm}}</td>
                          <td>{{this.vehicle_pylod}}</td>
                          <td><a href="javascript:void(0);"  class="edit_vehicle" data-id="{{this._id}}"><i class="fa fa-pencil"></i></i></a></td>
                          <td><a href="javascript:void(0);" class="delete_vehicle" data-id="{{this._id}}"><i class="fa fa-trash-o" style="color:red;"></a></td>
                      </tr>
                     {{/each}}  
                   </tbody>
                </table>
      </div>
    </div>
  </div>
      </section>
    </div>
    
  </div>
</div>
