{{> header}}
{{> nav}}


<div class="content-wrapper">
  <!-- Content Header (Page header) -->
  <section class="content-header">
     <a href="/product_mast/product_mast" style="color:white"><h1 class="btn btn-primary pull-left" style="margin-bottom: 21px;">Add Product Master</h1></a>
      <ol class="breadcrumb" >
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
         <li><a href="#"><i class="fa fa-dashboard"></i> Add Product Master</a></li>
      </ol>
    </section>
  <div class="row">
    <div class="col-md-7">
      <section class="content-header">
         
         
  {{!-- <div class="box box-primary"> --}}
    {{!-- <div class="box-header with-border"> --}}
      {{!-- <h3 class="box-title" id="add">Product Master</h3>
      <h3 class="box-title" id="edit" style="display: none">Update Product Master</h3> --}}
          {{#if err}}
          <div class="alert alert-danger">
          <strong>{{err}}</strong>
          </div>
          {{/if}}
          {{#if errors}}
          <div class="alert alert-danger">
          <strong>{{errors}}</strong>
          </div>
          {{/if}}
    {{!-- </div> --}}
  {{!-- </div> --}}
      </section>
    </div>
    <div class="col-md-12">
      <section class="content-header">
        <div class="box box-primary">
          <div class="box-header with-border">
            <div class="box-tools pull-right">
            <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
            <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-remove"></i></button>
          </div>
            <h3 class="box-title">View Product Master</h3>
                  <p style="text-align: center;color:#ff0000;margin-top: -20px;"></p>
          </div>
            <div class="box-body">
              <div class="table-responsive">
                <table id="example" class="table display table-bordered table-striped">
                  <thead>
                      <tr>
                        <th>#</th>
                        <th>Product Type Name</th>
                        <th>Item Code</th>
                        <th>Supressed Code</th>
                        <th>Artwork Rec Dt</th>
                        <th>Party Name</th>
                        <th>Material</th>
                        <th>Product Name</th>
                        <th>Board Quality</th>
                        <th>GSM (M/B)</th>
                        <th>Actual Size</th>
                        <th>GSM (Top/Middle/Bottom)</th>
                        <th>Pad Size</th>
                        <th>Leaf Color</th>
                        <th>Folding/Flat</th>
                        <th>Folding Pattern</th>
                        <th>Folding Size</th>
                        <th>Vertical/Horizontal</th>
                        <th>Image</th>
                        <th>Tucking size MM</th>
                        <th>Ear Flap</th>
                        <th>Design Style</th>
                        <th>Country</th>
                        <th>Board Paper Manufacturer</th>
                        <th>Construction</th>
                        <th>Emboss Rack No</th>
                        <th>Lamination</th>
                        <th>Lamination Type</th>
                        <th>UV</th>
                        <th>Varnish</th>
                        {{!-- Design --}}
                        <th>Pharma Code</th>
                        <th>Bar Code</th>
                        <th>Sheet / Paper Size</th>
                        <th>UV Blanket</th>
                        <th>Cut Size</th>
                        <th>Cut Size [inserts]</th>
                        <th>Reel Size</th>
                        <th>UPS</th>
                        <th>Grain</th>
                        <th>Die No.</th>
                        <th>Die Machine</th>
                        <th>Braille Type</th>
                        <th>Braille Rack No.</th>
                        <th>Blanket No</th>
                        <th>Gripper</th>
                        <th>Desgin Department</th>
                        <th>Blanket Information</th>
                        <th>QC Department</th>
                        <th width="20%">Action</th>
                        {{!-- <th width="10%">Delete</th> --}}
                      </tr>
                  </thead>
                 <tbody>
                   {{#each product as |value key|}}
                      <tr>
                          <td>{{sum @index 1}}</td>
                          <td>{{this.prdt_typ_name.prdt_typ_name}}</td>
                          <td>{{this.prdt_itemcode}}</td>
                          <td>{{this.prdt_subresscode}}</td>
                          <td>{{this.prdt_artwkdt}}</td>
                          <td>{{this.prdt_prtyname.party_name}}</td>
                          <td>{{this.prdt_material.accessories_desc}}</td>
                          <td>{{this.prdt_pname}}</td>
                          <td>{{this.prdt_brdqlty}}</td>
                          <td>{{this.prdt_gsmmb}}</td>
                          <td>{{this.actul_l}},{{this.actul_w}},{{this.actul_h}}</td>
                          <td>{{this.prdt_gsmtp}}</td>
                          <td>{{this.pad_l}},{{this.pad_w}},{{this.pad_h}}</td>
                          <td>{{this.prdt_lfcolor}}</td>
                          <td>{{this.prdt_fldingflat.indsrttyp_dscrpton}}</td>
                          <td>{{this.prdt_fldingpttrn.floading_desp}}</td>
                          <td>{{this.foldng_l}},{{this.foldng_w}},{{this.foldng_h}}</td>
                          <td>{{this.prdt_vrtclhorzntlone}},{{this.prdt_vrtclhorzntltwo}}</td>
                          <td><a href="{{this.filepath}}" target='_blank' alt="pdf Not Found"><img src="{{this.filepath}}" target="_blank" height="50" width="50"/></a></td>
                          <td>{{this.prdt_tkingszmm}}</td>
                          <td>{{this.prdt_erflp}}</td>
                          <td>{{this.prdt_dsignstyl.design_style}}</td>
                          <td>{{this.prdt_country.country_name}}</td>
                          <td>{{this.prdt_brdmnfctur.manufactur_name}}</td>
                          <td>{{this.prdt_cnstrcton.construction_name}}</td>
                          <td>{{this.prdt_embssrckno}}</td>
                          <td>{{this.prdt_lamination}}</td>
                          <td>{{this.prdt_laminationtyp}}</td>
                          <td>{{this.prdt_uv}}</td>
                          <td>{{this.prdt_vernish.vernish_name}}</td>
                          {{!-- <td>{{this.prdt_deprtmnt}}</td> --}}
                          <td>{{this.dsign_phrmacd}}</td>
                          <td>{{this.dsign_brcd}}</td>
                          <td>{{this.dsign_shetppsiz}}</td>
                          <td>{{this.dsign_uvblkt}}</td>
                          <td>{{this.dsign_ctsizone}},{{this.dsign_ctsiztwo}}</td>
                          <td>{{this.dsign_cutinsrtone}},{{this.dsign_cutinsrttwo}}</td>
                          <td>{{this.dsign_relsiz}}</td>
                          <td>{{this.dsign_ups}}</td>
                          <td>{{this.dsign_grain}}</td>
                          <td>{{this.dsign_dieno}}</td>
                          <td>{{this.design_michne.machine_name}}</td>
                          <td>{{this.dsign_bralletyp}}</td>
                          <td>{{this.dsign_brallerckno}}</td>
                          <td>{{this.dsign_blnktno}}</td>
                          <td>{{this.dsign_grippr}}</td>
                          {{!--Blanket  --}}
                         
                          <td><a href="/product_mast/product_mast_update/{{value._id}}"><button type="button" class="btn btn-info">Design Department</button></a></td>
                          <td><a href="/product_mast/product_mast_update/{{value._id}}"><button type="button" class="btn btn-danger">Blanket Information</button></a></td>
                          <td><a href="/product_mast/product_mast_update/{{value._id}}"><button type="button" class="btn btn-success">QC Department</button></a></a></td>
                          <td>
                             <div class="btn-group">
                          {{!-- <button type="button" class="btn btn-info">Action</button> --}}
                          <button type="button" class="btn btn-info dropdown-toggle" data-toggle="dropdown" aria-expanded="false">Action
                            <span class="caret"></span>
                            <span class="sr-only">Toggle Dropdown</span>
                          </button>
                          <ul class="dropdown-menu" role="menu">
                            <li><a href="/product_mast/product_mast_update/{{value._id}}"  class="edit_prodtmast" data-id="{{this._id}}"><i class="fa fa-pencil"></i></i></a></li>
                            <li><a href="/product_mast/delete_prodtmast/{{value._id}}" class="delbutton"><i class="fa fa-trash-o" style="color:red;"></i></a></li>
                            
                          </ul>
                        </div>
                       </td>
                      </tr>
                     {{/each}}  
                   </tbody>
                </table>
      </div>
    </div>
  </div>
      </section>
    </div>
    
  </div>
</div>
 {{> footer}}
