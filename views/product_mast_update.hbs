            <div class="col-md-8"> 
                <input type="text" class="form-control" name="dsign_dieno" id="dsign_dieno" value="{{product.dsign_dieno}}" placeholder="NA">
            </div>
            <label class="col-md-4">Die Machine</label>
            <div class="col-md-8">
              {{#if product.design_michne}}
               {{#listrt machine product.design_michne  'design_michne'}}
                            {{this.machine_name}}
                  {{/listrt}}
               {{else}}
               <select class="form-control" name="design_michne" id="design_michne">
                  <option value="">----Select Machine----</option>
                  {{#each machine}}
                  <option value="{{this._id}}" data-vernish="{{this._id}}">{{this.machine_name}}</option>
                  {{/each}}
              </select>
              {{/if}}
            </div>
